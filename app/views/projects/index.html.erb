<%= table_panel("LISTADO DE PROYECTOS") do %>
  <tr>
    <th>Estatus</th>
    <th>Nombre</th>
    <th>Nro Contrato</th>
    <th>Presupuesto</th>
    <th>Inicio</th>
    <th>Termino</th>
  </tr>
  <% @projects.each do |project| %>
    <tr>
      <td><%= project.state %></td>
      <td><%= project.name %></td>
      <td><%= project.contract_number %></td>
      <td><%= project.budget %></td>
      <td><%= project.init_date %></td>
      <td><%= project.finish_date %></td>
    </tr>
  <% end %>
<% end %>
